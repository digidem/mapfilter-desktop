{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.7.0" } }],
          "react"
        ],
        "plugins": ["transform-class-properties", "transform-es2015-classes"],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          ["env", { "targets": { "electron": "1.7.0" } }],
          "react"
        ],
        "plugins": ["transform-class-properties", "transform-es2015-classes"],
        "sourceMaps": "none"
      }
    }
  }
}

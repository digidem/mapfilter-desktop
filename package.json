{
  "name": "tizii-tizii",
  "productName": "TiziiTizii",
  "description": "electron app that wraps mapfilter",
  "author": "Digital Democracy",
  "version": "0.5.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/digidem/mapfilter-desktop.git"
  },
  "homepage": "https://github.com/digidem/mapfilter-desktop",
  "bugs": "https://github.com/digidem/mapfilter-desktop/issues",
  "main": "main/app.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "standard src",
    "check": "npm run check:used && npm run check:unused",
    "check:used": "dependency-check ./package.json --detective precinct -i electron --entry ./renderer/app.js",
    "check:unused": "dependency-check ./package.json --no-dev --detective precinct -i electron-debug --unused --entry ./renderer/app.js"
  },
  "license": "ISC",
  "keywords": [],
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "asar": true,
        "win32metadata": {
          "CompanyName": "TiziiTizii"
        }
      },
      "electronWinstallerConfig": {},
      "electronInstallerDebian": {
        "icon": "static/mapfilter.png",
        "categories": [
          "Utility"
        ]
      },
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "digidem",
        "name": "mapfilter-desktop",
        "draft": false
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "TiziiTizii"
      }
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "blob-to-buffer": "^1.2.6",
    "clone": "^2.1.1",
    "collect-stream": "^1.2.1",
    "drag-drop": "^2.13.2",
    "ecstatic": "^2.1.0",
    "electron-compile": "^6.4.2",
    "electron-config": "^0.2.1",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.1",
    "electron-is-dev": "^0.1.2",
    "electron-squirrel-startup": "^1.0.0",
    "from2": "^2.3.0",
    "get-ports": "^1.0.3",
    "glob": "^7.1.2",
    "hyperlog-sneakernet-replicator": "^1.1.4",
    "imghdr": "^0.1.6",
    "jpeg-marker-stream": "^1.0.0",
    "level": "^1.6.0",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.20",
    "material-ui-icons": "^1.0.0-beta.17",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "object-assign": "^4.1.1",
    "once": "^1.4.0",
    "osm-p2p": "^1.4.0",
    "osm-p2p-observations": "^2.0.0",
    "p2p-file-store": "^0.0.1",
    "prop-types": "^15.6.0",
    "pump": "^1.0.2",
    "randombytes": "^2.0.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "react-mapfilter": "^1.0.0-beta.1",
    "through2": "^2.0.3",
    "traverse": "^0.6.6",
    "xform-uploader": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "dependency-check": "^2.9.1",
    "devtron": "^1.4.0",
    "electron-forge": "^4.1.2",
    "electron-prebuilt-compile": "1.7.9",
    "precinct": "^3.8.0"
  }
}

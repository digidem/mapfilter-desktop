{
  "name": "tizii-tizii",
  "productName": "TiziiTizii",
  "description": "electron app that wraps mapfilter",
  "author": "Digital Democracy",
  "version": "0.6.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/digidem/mapfilter-desktop.git"
  },
  "homepage": "https://github.com/digidem/mapfilter-desktop",
  "bugs": "https://github.com/digidem/mapfilter-desktop/issues",
  "main": "main/app.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "standard src",
    "check": "npm run check:used && npm run check:unused",
    "check:used": "dependency-check ./package.json --detective precinct -i electron --entry ./renderer/app.js",
    "check:unused": "dependency-check ./package.json --no-dev --detective precinct -i electron-debug --unused --entry ./renderer/app.js"
  },
  "license": "ISC",
  "keywords": [],
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "asar": true,
        "win32metadata": {
          "CompanyName": "TiziiTizii"
        }
      },
      "electronWinstallerConfig": {},
      "electronInstallerDebian": {
        "icon": "static/mapfilter.png",
        "categories": [
          "Utility"
        ]
      },
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "digidem",
        "name": "mapfilter-desktop",
        "draft": false
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "TiziiTizii"
      }
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^1.1.1",
    "JSONStream": "^1.3.5",
    "blob-to-buffer": "^1.2.8",
    "clone": "^2.1.2",
    "collect-stream": "^1.2.1",
    "drag-drop": "^2.13.2",
    "ecstatic": "^2.1.0",
    "electron-compile": "^6.4.3",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^0.1.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^1.3.0",
    "from2": "^2.3.0",
    "get-ports": "^1.0.3",
    "glob": "^7.1.3",
    "imghdr": "^0.1.6",
    "jpeg-marker-stream": "^1.0.0",
    "lodash": "^4.17.11",
    "mapfilter-db": "^1.0.1",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "object-assign": "^4.1.1",
    "once": "^1.4.0",
    "prop-types": "^15.6.2",
    "pump": "^1.0.3",
    "randombytes": "^2.0.6",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-intl": "^2.7.2",
    "react-mapfilter": "^1.2.10",
    "through2": "^2.0.3",
    "traverse": "^0.6.6",
    "vips-resizer": "^2.1.0",
    "websocket-stream": "^5.1.2",
    "xform-uploader": "^1.0.0"
  },
  "devDependencies": {
    "@material-ui/codemod": "^1.1.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "d3-queue": "^3.0.7",
    "dependency-check": "^2.9.1",
    "devtron": "^1.4.0",
    "electron-forge": "^4.1.2",
    "electron-prebuilt-compile": "1.7.9",
    "precinct": "^3.8.0"
  }
}
